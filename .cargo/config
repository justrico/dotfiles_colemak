# 放到 `$HOME/.cargo/config` 文件中
[source.crates-io]
# registry = "https://github.com/rust-lang/crates.io-index"
# 指定镜像名，需与后面名称对应，如：tuna、sjtu、ustc，或者 rustcc
# replace-with = 'ustc'
replace-with = 'rsproxy'

# 中国科学技术大学
